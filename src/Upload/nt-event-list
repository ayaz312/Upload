   $upload_dir  = wp_upload_dir();
   // get venue image name 
   function get_venue_logo($id)
   {
   	$sq = "select * from club_nt_venues where id = $id";
   	$rst = mysql_query($sq);
   	$logo = mysql_result($rst , '0' , 'logo' );
   	return $logo;
   
   }
   
   
   
    // get venue name
   function get_venu_name_str($venue)
   {
   	
	$arr = explode(',',$venue);
	unset($name);
	foreach($arr as $key){
	
		$sq = "select * from club_nt_venues where id = $key";
		$rst = mysql_query($sq);
		$name[] = mysql_result($rst , '0' , 'name' );
	 }
   
   return $str = implode(',',$name);
   
   }
   
   
   // get venue name
   function get_venue_name($id)
   {
   	$sq = "select * from club_nt_venues where id = $id";
   	$rst = mysql_query($sq);
   	$name = mysql_result($rst , '0' , 'name' );
   	return $name;
   
   }
   
   function get_venue_location($id)
   {
		$sq = "select * from club_nt_venues where id = $id";
		$rst = mysql_query($sq);
		$location = mysql_result($rst , '0' , 'location' );
		$location = get_the_title($location);
		return $location;
   }
   
   
   	$num_rec_per_page = 6;
   	if (isset($_GET["custompage"])) { $page  = $_GET["custompage"]; } else { $page=1; }; 
   	$start_from = ($page-1) * $num_rec_per_page; 
   	$todaytime = date_i18n('Y-m-d H:i:s'); 
   	$con = 'where 1=1  ';

   	$sq = 'select TIME_TO_SEC(TIMEDIFF(cnt.event_date , "'.$todaytime.'")) as diff , cnt.* from club_nt as cnt  '.$con.' HAVING (diff > 0) order by event_date LIMIT '.$start_from.', '.$num_rec_per_page.' ';
   	$rst = mysql_query($sq);
   	$num = mysql_num_rows($rst);
   
