                              <?php
                                 $sq1_pagi = 'select TIME_TO_SEC(TIMEDIFF(cnt.event_date , "'.$todaytime.'")) as diff , cnt.* from club_nt as cnt '.$con.' HAVING (diff > 0)';
                                 $rs_result = mysql_query($sq1_pagi); //run the query
                                 $total_records = mysql_num_rows($rs_result);  //count number of records
                                 $total_pages = ceil($total_records / $num_rec_per_page); 
                                 $u =   get_bloginfo('url');
                                 $url = '';
                                 
                                 echo "<li><a class='pagi' href=' ".$u."/cc".$affiliate_ID."/?custompage=1".$url."'>".'|<'."</a></li> "; // Goto 1st page 
                                 for ($i=1; $i<=$total_pages; $i++) { 
                                 echo "<li><a class='pagi' href='".$u."/cc".$affiliate_ID."/?custompage=".$i."".$url."'>".$i."</a></li> "; 
                                 }; 
                                 
                                 echo "<li><a class='pagi' href='".$u."/cc".$affiliate_ID."/?custompage=$total_pages".$url."'>".'>|'."</a> </li>"; // Goto last page
                                 ?>
                           </ul>
                        </div>
                     </em>
                  </strong>
               </div>
               
               
               
<script>

$("#vcode").change(function(){
	
	if($('#vcode').val()!='')
	{
	$('.nextbtn').removeAttr('disabled');
	return true;
	}
	else if($('#vcode').val() =='')
	{
	$('.nextbtn').attr('disabled','disabled');
	return false;
	}
	$('#my_event_id').focus();
		
	
});

</script>               
               


               
               
<script type="text/javascript">

// this identifies your website in the createToken call below

 Stripe.setPublishableKey('<?php  echo $publishable_key = trim(get_post_meta('4189', 'publishable_key', true))?>');
//Stripe.setPublishableKey('pk_test_QVFtse9EIwiC2V2oNwbMKNLa');

function stripeResponseHandler(status, response) {
	
	if (response.error) {
	// re-enable the submit button
	$('.submit-button').removeAttr("disabled");
	// show the errors on the form
	$(".payment-errors").html(response.error.message);
	} else {
	var form$ = $("#payment-form");
	// token contains id, last4, and card type
	var token = response['id'];
	// insert the token into the form so it gets submitted to the server
	form$.append("<input type='hidden' name='stripeToken' value='" + token + "' />");
	// and submit
	//	form$.get(0).submit();

	// process card
	var fname = $('#fnamex').val();
	var lname = $('#lnamex').val();
	var phone = $('#phonex').val();
	var email = $('#email').val();
	var male = $('#malex').val();
	var female = $('#femalex').val();
	var total = $('#my_total_pricex').val();
	var affiliate_id = $('#user_affiliate_id').val();
	var event_id = $('#my_event_idx').val();

if(token !=''){
$('.close3btn').trigger('click');

	$.ajax({
	type: "POST",
	datatype: 'json',
	url: "<?php  bloginfo( 'template_url' ); ?>/NT/ntcc_stripe_charge_ajax.php",
	data: {male: male,female: female,fname: fname,lname: lname,email: email,phone: phone,affiliate_id: affiliate_id,event_id : event_id , amount : total , stripeToken:token},  
	
	success: function(data)
	{ 
		//alert(data);
		if(data == 1)
		{   
			$('#fname').val('');
			$('#lname').val('');
			$('#male').val('');
			$('#female').val('');
			$('#email').val('');
			$('#customer_phone').val('');
			$('#vcode').val('');
			$('#payment-form').trigger("reset");
			$('.success-message').text('Card has beec charged successfully.');
			$('.succesxt4').trigger('click');
			return true;
			
		}
		else if(data == 0)
		{
			alert('Something went wrong... Check your card information and try again.');
			return false;
		}
		else
		{
			
			alert(data);
			return false;
		}
		
	}
	
	});
}	
}
}
$(document).ready(function() {
$("#payment-form").submit(function(event) {
var msg = '';
if($('#fnamex').val()=='')
{
var msg = msg + "Please Enter First Name";
}
else if($('#lnamex').val()=='')
{
var msg = msg + "Please Enter Last Name";
}
else if($('#email').val()=='')
{
var msg = msg + "Please Enter Email";
}
else if($('#phonex').val()=='')
{
var msg = msg + "Please Enter Cell Phone";
}
else if($('#my_event_idx').val()=='')
{
var msg = msg + "Event Id is empty ";
}

else if($('#user_affiliate_id').val()=='')
{
	var msg = msg + "Refferal  Id is empty ";
}

else if($('#my_total_pricex').val()=='')
{
var msg = msg + "Please select for  how many  people are you purchasing ticket ?";
}
if(msg!='')
{
$(".payment-errors").html(msg);
return false;
}
// disable the submit button to prevent repeated clicks
$('.submit-button').attr("disabled", "disabled");
// createToken returns immediately - the supplied callback submits the form if there are no errors

var card_number = $('#mcard').val();
var card = card_number.replace(/\s+/g, '');

var expp =  $('.card-expiry-month').val()
var expp = expp.split('/');
var month = expp[0];
var year = expp[1];

 Stripe.createToken({
number: card,
cvc: $('.card-cvv').val(),
exp_month: month,
exp_year: year
}, stripeResponseHandler);

return false; // submit from callback



});
});
</script>              
               
<script>

$(".termsx").click(function(){

	$('.hidemodelx').trigger('click');
	
	// get val
	var id = this.id;
	var event_price = $('#event_price'+id).val();
	var event_venue = $('#event_venue'+id).val();
	var event_date = $('#event_date'+id).val();
	
	
	
	// set val
	$('.myidtxt').text(id);
	$('#my_event_id').val(id);
	$('#my_event_price').val(event_price);
	$('#my_event_venue').val(event_venue);
	
	
	
	$(".nextbtn").attr("id",id);
	$('.succesxt').trigger('click');

});



$(".nextbtn").click(function(){

	var id = this.id;
	
	var event_price = $('#event_price'+id).val();
	var event_venue = $('#event_venue'+id).val();
	var event_date = $('#event_date'+id).val();
	
	var vcode = $('#vcode').val();
	var phone = $('#customer_phone').val();
	var fname = $('#fname').val();
	var lname = $('#lname').val();
	
	var male = $('#male').val();
	var female = $('#female').val();
	
	if(male == '')
	{
		var male = 0;
	}
	else if(female == '')
	{
		var female = 0;
	}
	
	var total_people = parseInt(male) + parseInt(female);
	var total_price = total_people * parseInt(event_price);
	
	
	
	if(fname == '')
	{
		alert("Please enter your first name");
		return false;
	}
	
	
	else if(lname == '')
	{
		alert("Please enter your last name");
		return false;
	}
	
	else if(phone == '')
	{
		alert("Please enter your phone number");
		return false;
	}
	
	
	else if(male == '' && female == '')
	{
		alert("How many tickets?");
		return false;
	}
	
	else if(vcode == '')
	{
		alert("Please enter the verifcation code");
		return false;
	}
	
	
	
	$.ajax({
	type: "POST",
	datatype: 'json',
	url: "<?php  bloginfo( 'template_url' ); ?>/verify_code_ajax.php",
	data: {male: male,female: female,fname: fname,lname: lname,vcode: vcode,phone: phone,},  
	success: function(data)
	{ 
		
		if(data == 1)
		{
			$('.myclose').trigger('click');
			
			
			// refersh model 2 
			
			$('#fname').val('');
			$('#lname').val('');
			$('#male').val('');
			$('#female').val('');
			$('#customer_phone').val('');
			$('#vcode').val('');
			
			
			
			// add velue in model3
			$('#my_event_idx').val(id);
			$('#my_event_pricex').val(event_price);
			$('#my_event_venuex').val(event_venue);
			$('#my_event_datex').val(event_date);
			
			
			
			$('#malex').val(male);
			$('#femalex').val(female);
			
			$('#fnamex').val(fname);
			$('#lnamex').val(lname);
			$('#phonex').val(phone);
			
			$('#my_total_pricex').val(total_price);
			$('#payout').val('pay $'+total_price);
			
			// add text in model3
			$('.my_event_id_txt').text(id);
			$('.my_event_price_txt').text(event_price);
			$('.my_male_txt').text(male);
			$('.my_female_txt').text(female);
			$('.my_fname_txt').text(fname);
			$('.my_lname_txt').text(lname);
			$('.my_phone_txt').text(phone);
			$('.my_total_group_txt').text(total_people);
			$('.my_total_price_txt').text(total_price);
			$('.my_event_venue_txt').text(event_venue);
			$('.my_event_date_txt').text(event_date);
			
			$('.succesxt3').trigger('click');
			
		
			//alert(data);
		}
		else if(data == 0)
		{
			alert("verification code is not correct");
			$('#customer_phone').val('');
			$('#vcode').val('');
			$('.ntimagex').css('display','block');
		}
		else
		{
			alert(data);
			return false;
		}
		
	}
	
	});




});


$("#addtodb").click(function(){
    
	
	var fname = $('#fname').val();
	var lname = $('#lname').val();
	var phone = $('#customer_phone').val();
	
	if(fname == '')
	{
		alert("Please enter your First Name");
		return false;
	}
	else if(lname == '')
	{
		alert("Please enter your Last Name");
		return false;
	}
	
	else if(phone == '')
	{
		alert("Please enter your Phone Number ");
		return false;
	}
	
	//alert(phone);
	
	$.ajax({
	type: "POST",
	datatype: 'json',
	url: "<?php  bloginfo( 'template_url' ); ?>/nt_event_list_ajax.php",
	data: {fname: fname,lname: lname,phone: phone,},  
	success: function(data)
	{ 
		
		if(data == 1)
		{
			$('.ntimagex').css('display','none');
			$('.mytxt').text('Verification code was texted to you');
			return true;
		}
		else if(data == 0)
		{
		$('.ntimagex').css('display','block');
		$('.mytxt').text('Phone number is not valid !');
		return false;
		}
		else
		{
			
			alert(data);
			return false;
		}
		
	}
	
	});
	
	
	
});

</script>

<script>
            var maskList = $.masksSort($.masksLoad("<?php  bloginfo('template_url');?>/inputmask-multi-master/data/phone-codes.json"), ['#'], /[0-9]|#/, "mask");
            var maskOpts = {
                inputmask: {
                    definitions: {
                        '#': {
                            validator: "[0-9]",
                            cardinality: 1
                        }
                    },
                    showMaskOnHover: false,
                    autoUnmask: true,
                    clearMaskOnLostFocus: false
                },
                match: /[0-9]/,
                replace: '#',
                list: maskList,
                listKey: "mask",
                onMaskChange: function(maskObj, determined) {
                    if (determined) {
                        var hint = maskObj.name_en;
                        if (maskObj.desc_en && maskObj.desc_en != "") {
                            hint += " (" + maskObj.desc_en + ")";
                        }
                        $("#descr").html(hint);
                    } else {
                        $("#descr").html("Country code & phone#");
                    }
                }
            };

            $('#phone_mask').change(function() {
                if ($('#phone_mask').is(':checked')) {
                    $('#customer_phone').inputmask("remove");
                    $('#customer_phone').inputmasks(maskOpts);
                } else {
                    $('#customer_phone').inputmasks("remove");
                    $('#customer_phone').inputmask("+#{*}", maskOpts.inputmask);
                    $("#descr").html("Country code & phone#");
                }
            });

            $('#phone_mask').change();
        </script>










